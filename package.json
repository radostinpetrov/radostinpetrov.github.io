{
  "name": "my-personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild:export": "if [ \"$STATIC_EXPORT\" = \"true\" ] || [ \"$GITHUB_PAGES\" = \"true\" ]; then mv app/api app_api_tmp || true; fi",
    "build": "npm run prebuild:export && next build && npm run postbuild:export",
    "postbuild:export": "if [ \"$STATIC_EXPORT\" = \"true\" ] || [ \"$GITHUB_PAGES\" = \"true\" ]; then touch out/.nojekyll; mv app_api_tmp app/api || true; fi",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@vercel/blob": "^1.1.1",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.510.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-datetime": "^3.3.1",
    "react-dom": "^19.0.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
